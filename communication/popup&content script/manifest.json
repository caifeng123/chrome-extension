{
	"name": "popup&content script",
	"description": "example for communication between popup and content script",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["third/jquery.js","tiaoseban.js","content script.js"]
		}
	],
	"action": {
		"default_popup": "index.html"
	}
}
