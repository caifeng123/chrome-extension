{
	"name": "Getting Started Example",
	"description": "Build an Extension!",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["activeTab", "scripting"],
	"devtools_page": "popup.html",
	"action": {
		"default_popup": "1.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"host_permissions": ["http://*/*", "https://*/*"]
}
