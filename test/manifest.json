{
	"name": "Getting Started Example",
	"description": "Build an Extension!",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "test.js"
	},
	"permissions": ["activeTab", "scripting"],
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content-script.js"]
		}
	]
}
